{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "antd": "^4.1.0",
    "apollo-boost": "^0.4.7",
    "apollo-client": "^2.6.8",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-styled-components": "^1.10.7",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "next": "9.3.2",
    "next-with-apollo": "^5.0.0",
    "null-loader": "^3.0.0",
    "react": "16.13.1",
    "react-apollo": "^3.1.3",
    "react-dom": "16.13.1",
    "styled-components": "^5.0.1"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ],
          [
            "import",
            {
              "libraryName": "antd",
              "style": "css"
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ],
          [
            "import",
            {
              "libraryName": "antd",
              "style": "css"
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ],
          [
            "import",
            {
              "libraryName": "antd",
              "style": "css"
            }
          ]
        ]
      }
    }
  }
}
